package invoicing;
// classes are blueprint/ basic structure
public class Product {

	// pid, pname, description, price
	int pid;
	String pname;// laptop , bluetooth
	String description;
	double price;
	
	void initialize(int pid, String pname, 
			String description, double price)
	{
		pid = 1;
		pname = "Laptop";
		description ="Apple MAC PRO";
		price = 80000;
	}
}
