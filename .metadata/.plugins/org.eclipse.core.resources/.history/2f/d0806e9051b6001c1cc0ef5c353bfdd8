
public class ArrayDemo {

	public static void main(String[] args) {
		
		// declare, create and initialize , collections in java 
		int arr[] = new int[5]; // declare and create 5 integer value => 5 * 4 = 20 bytes
		int arr1[] = {1,2,3,4,5}; // delare, create and initialize
		int arr2[];// declare
		arr2 = new int[5];// creation
		int arr3[] = new int[] {1,2,3,4,5};//0 to 4
		
		System.out.println(arr3[0]);//1
		System.out.println(arr2[0]);// 
		//System.out.println(arr[5]);// garbage
		
		String names[] = new String[5];
		System.out.println(names[0]);
		
	}

}
