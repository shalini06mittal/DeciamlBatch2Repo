package invoicing.datastore;

import invoicing.model.Customer;

public class Database {
	
	public Customer[] createCustomer()
	{
		Customer[] customers = new Customer[3];
		customers[0] = new Customer(1, "Shalni", "India", "shalini@gmail.com","2323123", "shalini", "GST-123", "Mumnbai");
		customers[1] = new Customer(2, "abc", "India", "abc@gmail.com","2323123", "shalini", "GST-123", "Mumnbai");
		customers[2] = new Customer(3, "xyz", "India", "xyz@gmail.com","2323123", "shalini", "GST-123", "Mumnbai");
		return customers;
	}
 
}
